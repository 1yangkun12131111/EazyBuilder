"8" width="49"/><column col-number="9" width="40"/><column col-number="10" width="87"/><column col-number="11" width="46"/><column col-number="12" width="36"/><column col-number="13" width="41"/><column col-number="14" width="40"/><column col-number="15" width="64"/><column col-number="16" width="62"/><column col-number="17" width="66"/><column col-number="18" width="67"/><column col-number="19" width="48"/><datasource name="136-ci-mysql" type="jdbc" username="useranme" password="password" url="jdbc:mysql://0.0.0.0/ci?useUnicode=true&amp;characterEncoding=utf-8" driver="com.mysql.jdbc.Driver"><dataset name="team-activity" type="sql"><sql><![CDATA[SELECT
    t.code AS teamCode,
    t.name AS teamName,
      p.id as pid,
    p.name,
    p.description,
    ifnull(st.email,'N/A') as email,
    ifnull(sum(st.additions),0) as totalAdd,
    ifnull(sum(st.deletions),0) as totalDel,
    ifnull(sum(st.pushed),0) as totalPush
FROM
    ci_project p
    inner join ci_team t on p.team_id = t.id
    inner join ci_scm s on p.scm_id = s.id
	left join ci_user_statistic st on s.url LIKE CONCAT('%',
            st.group_name,
            '/',
            st.project_name,
            '%')
WHERE 1=1
and st.day is null or (st.day >= :startDate and st.day <= :endDate)
group by teamCode,teamName,pid,name,description,st.email 
ORDER BY teamCode , name ]]></sql><field name="teamCode"/><field name="teamName"/><field name="pid"/><field name="name"/><field name="description"/><field name="email"/><field name="totalAdd"/><field name="totalDel"/><field name="totalPush"/><parameter name="startDate" type="String" default-value="2021-01-01"/><parameter name="endDate" type="String" default-value="2021-12-31"/></dataset><dataset name="projectQuality" type="sql"><sql><![CDATA[SELECT 
    t.code AS teamCode,
    t.name AS teamName,
    prj.id as pid,
    prj.name AS projectName,
    prj.description AS projectDesc,
	sum(if(m.type='code_line',m.val,0)) as '代码行',
    sum(if(m.type='bug_blocker',m.val,0)) as 'BUG阻断',
	sum(if(m.type='bug_critical',m.val,0)) as 'BUG严重',
    sum(if(m.type='code_smell_blocker',m.val,0)) as '代码规范(阻断)',
    sum(if(m.type='code_smell_critical',m.val,0)) as '代码规范(严重)',
	sum(if(m.type='vulner_blocker',m.val,0)) as '安全漏洞（阻断）',
    sum(if(m.type='vulner_critical',m.val,0)) as '安全漏洞(严重)',
     sum(if(m.type='vulner_critical',m.val,0)) as '安全漏洞(严重)',
      sum(if(m.type='unit_test_success_rate',m.val,0)) as '单元测试成功率',
      sum(if(m.type='unit_test_coverage_rate',m.val,0)) as '单元测试覆盖率',
      sum(if(m.type='dc_high',m.val,0)) as '依赖漏洞(高危)',
     sum(if(m.type='dc_medium',m.val,0)) as '依赖漏洞(中等)'
FROM
    ci_project prj
    left join
    (SELECT 
        MAX(a.id) AS mid, a.type AS metricType, b.project_id
    FROM
        ci_metric a, ci_pipeline_history b
    WHERE
        a.pipeline_id = b.id
	and b.start_time_millis >= unix_timestamp(:startDate)*1000 and b.start_time_millis <= unix_timestamp(:endDate)*1000
    GROUP BY b.project_id , metricType) p on p.project_id=prj.id 
    inner join ci_team t on prj.team_id=t.id
    left join ci_metric m on p.mid=m.id

GROUP BY teamCode , teamName , pid,projectName , projectDesc 
]]></sql><field name="teamCode"/><field name="teamName"/><field name="pid"/><field name="projectName"/><field name="projectDesc"/><field name="代码行"/><field name="BUG阻断"/><field name="BUG严重"/><field name="代码规范(阻断)"/><field name="代码规范(严重)"/><field name="安全漏洞（阻断）"/><field name="安全漏洞(严重)"/><field name="安全漏洞(严重)"/><field name="单元测试成功率"/><field name="单元测试覆盖率"/><field name="依赖漏洞(高危)"/><field name="依赖漏洞(中等)"/><parameter name="startDate" type="String" default-value="2021-1-1"/><parameter name="endDate" type="String" default-value="2021-12-31"/></dataset><dataset name="users" type="sql"><sql><![CDATA[select b.id as userId,b.name as userName from ci_user b]]></sql><field name="userId"/><field name="userName"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
